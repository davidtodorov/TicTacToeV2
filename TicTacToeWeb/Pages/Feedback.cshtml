@page
@model TicTacToeWeb.Pages.FeedbackModel
@{
    ViewData["Title"] = "Feedback";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>You can leave your feedback here:</h2>
<div class="row" style="margin-top: 30px;">
    <form method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="container">
            @Html.EditorForModel()
            <button type="submit" class="btn btn-default create-game-btn">Submit</button>
        </div>
    </form>
</div>
